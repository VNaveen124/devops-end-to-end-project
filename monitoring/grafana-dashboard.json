    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "CPU Usage",
      "type": "graph",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{namespace=\"three-tier-app\"}[5m])) by (pod)"
        }
      ]
    },
    {
      "title": "Memory Usage",
      "type": "graph",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(container_memory_usage_bytes{namespace=\"three-tier-app\"}) by (pod)"
        }
      ]
    },
    {
      "title": "Request Latency",
      "type": "graph",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 9
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, pod))"
        }
      ]
    },
    {
      "title": "DB Connections",
      "type": "graph",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 9
      },
      "targets": [
        {
          "expr": "sum(mysql_global_status_threads_connected)"
        }
      ]
    }
  ],
